<krpano>
    <layer name="btn_fs" url="%HTMLPATH%//skin/baseline_fullscreen_black_48dp.png" keep="true" align="righttop"
        alpha="0.67"
        scale="0.8"
        onclick="toggle(fullscreen);"
        onover="tween(alpha, 0.8, 0.2);tween(scale, 1, 0.2)"
        onout="tween(alpha, 0.67, 0.2);tween(scale, 0.8, 0.2)"
    />
    <events name="fs_events" keep="true"
        onenterfullscreen="set(layer['btn_fs'].url, %HTMLPATH%//skin/baseline_fullscreen_exit_black_48dp.png)"
        onexitfullscreen="set(layer['btn_fs'].url, %HTMLPATH%//skin/baseline_fullscreen_black_48dp.png)"
    />
</krpano>